<config>
    <tabs>
        <factoryx translate="label" module="lookbook">
            <label>Factory X</label>
            <sort_order>101</sort_order>
        </factoryx>
    </tabs>

    <sections>
        <lookbook translate="label" module="lookbook">
            <label>Lookbook</label>
            <tab>factoryx</tab>
            <sort_order>10</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <options translate="label" module="lookbook">
                    <label>Options</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enable_jquery translate="label">
                            <label>Enable jQuery</label>
                            <frontend_type>select</frontend_type>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                        </enable_jquery>
                    </fields>
                </options>
            </groups>
        </lookbook>
    </sections>

</config>
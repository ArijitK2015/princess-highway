<?xml version="1.0"?>
<config>
    <modules>
        <FactoryX_Promo>
            <version>0.0.6</version>
        </FactoryX_Promo>
    </modules>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <promo before="Mage_Adminhtml">FactoryX_Promo</promo>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <global>
        <blocks>
            <adminhtml>
                <rewrite>
                      <promo_catalog_grid>FactoryX_Promo_Block_Adminhtml_Catalog_Grid</promo_catalog_grid>
                      <promo_quote_grid>FactoryX_Promo_Block_Adminhtml_Quote_Grid</promo_quote_grid>
                </rewrite>
            </adminhtml>
        </blocks>
    </global>
    <phpunit>
        <suite>
            <modules>
                <FactoryX_Promo/>
            </modules>
        </suite>
    </phpunit>
</config>